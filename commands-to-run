git clone https://github.com/openshift/openshift-ansible.git
git checkout release-3.9

aws cloudformation create-stack --region us-east-1  --stack-name sdb-ocp --template-url https://s3.amazonaws.com/ocp-cloud-formation-template/CloudFormationTemplateOpenShift.yaml --parameters ParameterKey=AvailabilityZone,ParameterValue=us-east-1e ParameterKey=KeyName,ParameterValue=kp2 --capabilities=CAPABILITY_IAM

ansible-playbook prepare.yml -i ./hosts.yml --key-file kp2.pem

ansible-playbook -i hosts.yml openshift-ansible/playbooks/prerequisites.yml --key-file kp2.pem

ansible-playbook -i hosts.yml openshift-ansible/playbooks/deploy_cluster.yml --key-file kp2.pem